<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <!-- Adiciona permissÃ£o de internet -->
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ecotrack"
        android:usesCleartextTraffic="true"
        tools:targetApi="31">

        <activity
            android:name=".InicialActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LoginActivity"
            android:exported="false" />

        <activity
            android:name=".CadastroActivity"
            android:exported="false" />

        <activity
            android:name=".DicaLuzActivity"
            android:exported="false" />

        <activity
            android:name=".MenuActivity"
            android:exported="false" />

        <activity
            android:name=".MeuConsumoActivity"
            android:exported="false" />

        <activity
            android:name=".CadastroEletrodomesticosActivity"
            android:exported="false" />

        <activity
            android:name=".RelatorioConsumoActivity"
            android:exported="false" />

        <activity
            android:name=".ComoUsarActivity"
            android:exported="false" />

        <activity
            android:name=".DicasActivity"
            android:exported="false" />

        <activity
            android:name=".DicaGeladeiraActivity"
            android:exported="false" />

        <activity
            android:name=".DicaTelevisaoActivity"
            android:exported="false" />

        <activity
            android:name=".DicaMicroondasActivity"
            android:exported="false" />

        <activity
            android:name=".DicaChuveiroActivity"
            android:exported="false" />

        <activity
            android:name=".DicaFogaoActivity"
            android:exported="false" />

        <activity
            android:name=".DicaLuzActivity"
            android:exported="false"
            tools:ignore="DuplicateActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>